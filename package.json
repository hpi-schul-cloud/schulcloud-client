    "express-session": "^1.15.2",
    "fast-csv": "^2.4.1",
    "feedr": "^2.13.5",
    "handlebars-layouts": "^3.1.4",
    "handlebars-wax": "^5.1.1",
    "hbs": "~4.0.1",
    "ipaddr.js": "^1.5.4",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "maxmind": "^2.3.0",
    "method-override": "^2.3.7",
    "moment": "^2.17.1",
    "morgan": "~1.7.0",
    "multer": "^1.3.0",
    "nexboard-api-js": "^1.0.5",
    "oauth-1.0a": "^2.1.0",
