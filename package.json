{
  "name": "schulcloud-client",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon --watch ./ --watch views/ --watch controllers/ --watch build/ --ext js,hbs ./bin/www",
    "debug": "nodemon --inspect=0.0.0.0:9310 --watch controllers/ --watch helpers/ --watch views/ --ext js,hbs ./bin/www",
    "lint": "eslint ./bin ./controllers ./helpers ./test ./api.js ./app.js --ext .js --fix",
    "startd": "echo 'Do you mean 'npm run watch'?'",
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha test/ --recursive",
    "coverage": "NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- -- test/**/*.test.js"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
