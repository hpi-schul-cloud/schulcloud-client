    "gulp-imagemin": "^3.1.1",
    "gulp-optimize-js": "^1.1.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rimraf": "^0.2.1",
    "gulp-sass": "git+https://github.com/schul-cloud/gulp-sass.git",
    "gulp-uglify": "^2.0.1",
