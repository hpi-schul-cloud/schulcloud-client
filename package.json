{
  "name": "schulcloud-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www -w ./",
    "lint": "eslint ./bin ./controllers ./helpers ./test ./api.js ./app.js --ext .js --fix",
    "startd": "echo 'Do you mean 'npm run watch'?'",
    "test": "npm run lint && npm run mocha",
    "mocha": "mocha test/ --recursive",
    "coverage": "NODE_ENV=test istanbul cover node_modules/.bin/_mocha -- -- test/**/*.test.js"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "compression": "^1.6.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "handlebars-layouts": "^3.1.4",
    "handlebars-wax": "^5.1.1",
    "hbs": "~4.0.1",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "method-override": "^2.3.7",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "morgan": "~1.7.0",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "clean-css": "^4.0.8",
    "eslint": "^3.17.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.0.3",
    "gulp-csso": "^2.0.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-rimraf": "^0.2.1",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.0.1",
    "gulp-util": "^3.0.8",
    "pump": "^1.0.2",
    "vinyl-map": "^1.0.2"
  }
}
