<h2 class="h4">{{title}}</h2>

<div class="card-columns">

    {{#each content.resources}}
        <div class="card">
            <div class="card-block">
                <h4 class="card-title">
                    <a href="{{url}}" target="_blank">
                        {{title}}
                    </a>
                </h4>
                <p class="card-text">{{description}}</p>
            </div>
            <div class="card-footer">
                <small class="text-muted">via {{client}}</small>
            </div>
        </div>
    {{/each}}

</div>
