{{#extend "lib/loggedin"}}

    {{#content "styles" mode="append"}}
        <link rel="stylesheet" href="/styles/help/help.css"/>
    {{/content}}

    {{#content "scripts" mode="append"}}
        <script src="/scripts/help.js" type="text/javascript" defer></script>
    {{/content}}

    {{#content "page"}}
        
        {{> "help/search"}}
        
        {{#embed "help/component-card" title="Kontaktformular" icon="fa-pencil" class="contact-card"}}
            {{#content "card-body"}}
                {{> "help/contact"}}
            {{/content}}
        {{/embed}}

    {{/content}}
{{/extend}}
