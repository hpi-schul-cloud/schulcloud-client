{{#extend "lib/loggedin"}}

{{#content "styles" mode="append"}}
<link rel="stylesheet" href="{{getAssetPath '/styles/help/help.css'}}" />
{{/content}}

{{#content "scripts" mode="append"}}
<script src="{{getAssetPath '/scripts/help.js'}}" type="text/javascript" nonce="{{nonceValue}}" defer></script>
{{/content}}

{{#content "page"}}

{{> "help/search" class="md-2 hidden"}}

<div class="container">
  <div class="row">
    {{#embed "help/icon-card" id="contact-form" title=($t "help.partials.headline.contactForm") icon="fa-pencil" class="contact-card col-md-12" id="kontakt"}}
      {{#content "card-body"}}
        {{> "help/contact-card"}}
      {{/content}}
    {{/embed}}
  </div>
</div>
{{/content}}
{{/extend}}
