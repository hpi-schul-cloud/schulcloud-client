<input name="schoolId" type="hidden" data-force-value="true" value="{{currentSchool}}" />

<div class="form-group">
    <label>{{$t "administration.teams.label.selectAnAdditionalOwner" }}</label>
    {{{course.name}}}
    <select class="form-control form-control-lg search-enabled" type="text" name="userId">
        <option value="">{{$t "administration.teams.button.addAnotherOwner" }}</option>
        {{#each users}}
            <option value="{{{this._id}}}">
                {{this.lastName}} {{this.firstName}}
            </option>
        {{/each}}
    </select>
</div>
