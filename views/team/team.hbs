<section class="section-team">
            {{#each teams}}
                <h5>{{name}}</h5>
            <div class="container">
                <div class="row">
            {{#each this.team}}
                <div id="modal-container-{{id}}">
                    <div class="modal-background {{#if ../../loggedin}}modal-padding{{/if}}">
                        <div class="modal fade" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
                            <div class="modal-dialog" style="margin-top: {{#if longDesc}} {{ longDesc }} {{else}} 20vh {{/if}}" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <i class="fa fa-times" aria-hidden="true"></i>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <div data-tilt data-tilt-glare data-tilt-max-glare="0.8">
                                                    <img class="image" src="{{pictureUrl}}">
                                                </div>
                                            </div>
                                            <div class="col-md-2"></div>
                                            <div class="col-md-7">
                                                <h2 class="person-title" style="text-align: left">{{title}} {{firstName}} {{lastName}}</h2>
                                                <h5 class="person-desc" style="text-align: left">{{position}}</h5>
                                                <p style="text-align: left">{{{description}}}</p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="content col-md-4 col-sm-6 col-xs-12">
                    <div class="person-wrapper">
                        <div class="buttons">
                            <div id="two" class="button{{id}}">
                                <div class="card h-100 person">
                                    <img class="card-img-top circular" src="{{pictureUrl}}">
                                    <div class="card-block">
                                        <h4 class="person-title card-title">{{firstName}} {{lastName}}</h4>
                                        <p class="person-desc card-text">{{position}}</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {{/each}}
                </div>
            </div>
            {{/each}}
        <!-- </div>
    </div> -->
</section>